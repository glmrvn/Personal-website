.br
- cache ['store', Article.latest] do
  - @articles.web_tag.image_have.order('created_at ASC').each do |article|
    - cache ['entry', article] do
      = image_tag(article.image_url, class: "art_img_web")
.br

- @articles.app_tag.image_have.order('created_at ASC').each do |article_app|
  = image_tag(article_app.image_url, class: "art_img_app")
